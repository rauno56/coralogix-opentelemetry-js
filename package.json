    "@opentelemetry/semantic-conventions": "^1.25.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.8.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "tsup": "^7.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2"
  }
